<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="dht11_partner" model="res.partner">
            <field name="company_id" ref="base.main_company"/>
            <field name="name">DHT11</field>
            <field name="display_name">DHT11</field>
        </record>
        <record id="dht11_user" model="res.users">
            <field name="company_id" ref="base.main_company"/>
            <field name="partner_id" ref="dht11_partner"/>
            <field name="active" eval="True"/>
            <field name="login">dht11login</field>
            <field name="password">dht11password</field>
        </record>
    </data>
</odoo>